<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="688px" android:layout_width="200px" android:background="@drawable/edit_collage_photo_bg">

<com.beyondsoft.giinii.collage.PhotoListGridView 
	android:id="@+id/edit_collage_photo_listview" 
	android:layout_alignParentTop="true"
	android:verticalSpacing="10px" 
	android:layout_marginTop="100px" 
	android:layout_marginBottom="110px" 
	android:layout_height="wrap_content" 
	android:layout_width="196px" 
	android:paddingLeft="53px" android:paddingRight="53px">
</com.beyondsoft.giinii.collage.PhotoListGridView>

<ImageView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/edit_collage_photohidebtn" android:src="@drawable/collage_hidebtn_selector" android:layout_alignParentRight="true" android:layout_alignParentTop="true" android:layout_marginTop="25px" android:layout_marginRight="21px"></ImageView><ImageView 
	android:id="@+id/edit_collage_addtophotobtn" 
	android:layout_alignParentBottom="true" 
	android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/collage_addbtn_selector" android:layout_centerHorizontal="true" android:layout_marginBottom="20px">
</ImageView>



</RelativeLayout>
