<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="1024px" android:layout_height="25px"
	android:orientation="horizontal">

	<TextView android:id="@+id/pagename" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Photo Album"
		android:textStyle="bold" android:paddingLeft="10px" android:textSize="16px" />

	<TextView android:id="@+id/time" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:paddingLeft="700px"
		android:textStyle="bold" android:textSize="16px" />

	<ImageView android:id="@+id/signal" android:layout_width="wrap_content"
		android:paddingTop="5px" android:paddingLeft="15px"
		android:layout_height="wrap_content" android:src="@drawable/wifi_signal_open" />
	<TextView android:id="@+id/powerStatus" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textStyle="bold"
		android:textSize="16px" android:paddingLeft="15px" />
	<ImageView android:id="@+id/tvPower" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_marginLeft="15px"
		android:layout_marginTop="8px" />

</LinearLayout>